@misc{mondal_postnikov_2024,
	title = {On Postnikov completeness for replete topoi},
	url = {http://arxiv.org/abs/2210.14146},
	doi = {10.48550/arXiv.2210.14146},
	abstract = {We show that the hypercomplete \${\textbackslash}infty\$-topos associated with any replete topos is Postnikov complete, positively answering a question of Bhatt and Scholze; this will be deduced from the Milnor sequences for sheaves of spaces on replete topoi that we construct. As a corollary, we generalize a result of To{\textbackslash}"en on affine stacks.},
	number = {{arXiv}:2210.14146},
	publisher = {{arXiv}},
	author = {Mondal, Shubhodip and Reinecke, Emanuel},
	urldate = {2024-12-27},
	date = {2024-05-27},
	eprinttype = {arxiv},
	eprint = {2210.14146 [math]},
	keywords = {Mathematics - Algebraic Geometry, Mathematics - Algebraic Topology},
}

